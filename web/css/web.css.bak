/* 测试 **************/

/* 公共属性 ********************************/
#top{
	width:100%;
	margin:0 auto;
	float:left;
}
#nav,#main,#footer{
	width:100%;
	margin:0 auto;
}
body{
	font:12px "microsoft yahei",Arial,sans-serif;
	color:#666;
	background-color:#FAFAFA;
	margin:0;
	padding:0;
}
ul,p,h1,h2,h3,h4,h5,h6,dl,dd{
	margin:0;
	padding:0;
	list-style:none;
	font-style: normal;
    font-size: 100%;
}
input{border:0;}
.fl{float:left;}
.fr{float:right;}
.clear{clear:both;}
img{vertical-align:bottom;}
a{
	color: #CBCBCB;
	text-decoration:none;
}
h3.title{
	border:0;
	border-left:5px solid #3498DB;
	padding-left:5px;
	font-weight:bold;
	font-size:19px;
	margin-top:20px;
}
/* a:hover{
	color:#ff0700;
	text-decoration:underline;
} */
/* STPE1:顶部top *******************************/
#top{
	background-color:#42464C;
	margin-bottom: 3em;
}
/* 左侧 **********/
#top .w3cfuns{
	display: block;
    height: 40px;
    width: 155px;
    margin: 5px 20px;
	float:left;
}
#top img{
	width:60px;
	height:39px;
	float:left;
}
#top .w3cfuns span{
	display: block;
    line-height: 39px;
    color: #fff;
    float: left;
    margin-left: 15px;
    font-size: 22px;
    font-weight: bold;
}
#top .list{
	float: left;
    line-height: 30px;
}
.list li,.shorNav li {
	float:left;
	padding:10px 13px;
	color:#CBCBCB;
}
.list li a{
	color: #CBCBCB;
	font-size: 15px;
}
.list li a:hover,#top .shortNav li a:hover{
	color:#6BC30D;
	text-decoration:none;
} 
.list .firstPage{
	background-color:#333333;
}
.list .firstPage a{
	color:#6BC30D;
}
/* 右侧 **********/
#top .shortNav{
	float: right;
    line-height: 30px;
}
#top .shortNav li{
	float: left;
    padding: 10px 15px;
    color: #CBCBCB;
    font-size: 15px;
}
.shortNav .search{
	position:relative;
}
